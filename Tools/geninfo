#!/bin/sh

NXTVER=`expr $MINVER + 1`

cat << EOF
// Autogenerated by geninfo, do not edit

package $MODPKGNAME;

public class Info {
	static final String modName = "$MODTITLE";
	static final String modID = "$MODID";
	//static final String modPackage = "$MODSUBPKG";
	static final String versionNumber = "$MODVER";
	static final String versionBounds = "[$MAJVER.$MINVER,$MAJVER.$NXTVER)";
}
EOF
